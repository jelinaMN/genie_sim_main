(define (problem pass_water_to_people)
    (:domain isaacsim)

    (:objects
        benchmark_beverage_bottle_001 benchmark_beverage_bottle_008 - bottle.n.01
    )

    (:init
        (onfloor benchmark_beverage_bottle_001 floor.n.01_2)
    )

    (:goal
        (forn 1 (?bottle.n.01 - bottle.n.01)
            (pass2people ?bottle.n.01)
        )
    )
)